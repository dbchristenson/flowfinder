{
    "manifest_version": 3,
    "name": "Flow Finder",
    "version": "1.0",
    "description": "Find your flow, unblock your masterpiece.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "dist/background.js"
    },
    "action": {
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "src/resources/icon16.png",
            "48": "src/resources/icon48.png",
            "128": "src/resources/icon128.png"
        }
    },
    "icons": {
        "16": "src/resources/icon16.png",
        "48": "src/resources/icon48.png",
        "128": "src/resources/icon128.png"
    }
}